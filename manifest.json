{
  "manifest_version": 3,
  "name": "Gulf of Mexico",
  "version": "1.1",
  "description": "Forces Google Maps to display 'Gulf of Mexico'.",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/maps/*",
        "https://maps.google.com/maps/*"
      ],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
